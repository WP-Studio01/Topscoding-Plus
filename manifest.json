{
	"manifest_version": 3,
	"name": "Topscoding plus",
	"version": "1.1.4", 
	"content_scripts": [
		{
			"matches": ["https://www.topscoding.com/*"],
			"js": ["marked.min.js","jquery.min.js","index.js"]
		}
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": "topscoding.png",
		"default_title": "Topscoding Plus"
	},
	"icons": {
		"16": "topscoding.png",
		"32": "topscoding.png",
		"48": "topscoding.png",
		"128": "topscoding.png"
	},
	"homepage_url": "https://www.github.com/WP-Studio01/TopscodingPlus/",
	"permissions": [
		"storage",
		"tabs"
	]
}